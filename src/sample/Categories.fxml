<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<SplitPane xmlns:fx="http://javafx.com/fxml/1" dividerPositions="0.25" prefHeight="316.0" prefWidth="780.0"
           xmlns="http://javafx.com/javafx/8.0.171" fx:controller="sample.controllers.CategoriesController">
    <items>
        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="247.0" prefWidth="195.0">
            <children>
                <TextField fx:id="TxtSearch" disable="true" layoutX="56.0" layoutY="23.0" prefHeight="25.0"
                           prefWidth="122.0"/>
                <Label layoutX="8.0" layoutY="25.0" text="Search:">
                    <font>
                        <Font size="14.0"/>
                    </font>
                </Label>
                <Button fx:id="BtnAddToInvoice" disable="true" layoutX="42.0" layoutY="78.0" mnemonicParsing="false"
                        onAction="#moveItemToInvoice" prefHeight="35.0" prefWidth="114.0" text="Add To Invoice"
                        wrapText="true"/>
                <Button fx:id="BtnResetItems" disable="true" layoutX="39.0" layoutY="142.0" mnemonicParsing="false"
                        onAction="#resetItems" prefHeight="35.0" prefWidth="114.0" text="Reset items" wrapText="true"/>
            </children>
        </AnchorPane>
        <TabPane fx:id="Tables">

            <tabs>
            </tabs>
        </TabPane>
    </items>
</SplitPane>
